{
  "name": "vite-plugin-livescript",
  "version": "0.0.0",
  "description": "A Vite plugin for your LiveScript projects",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "lsc --output dist index.ls",
    "prepack": "npm run build",
    "release": "np --no-yarn --any-branch",
    "test": "npm-run-all --parallel test:*",
    "test:javascript": "cd tests && vite build --config vite-javascript.config.js",
    "test:livescript": "cd tests && vite build --config vite-livescript.config.js",
    "test:typescript": "cd tests && vite build --config vite-typescript.config.js"
  },
  "keywords": [
    "vite-plugin",
    "livescript"
  ],
  "author": "Jan T. Sott",
  "license": "MIT",
  "dependencies": {
    "livescript": "^1.6.0"
  },
  "devDependencies": {
    "np": "^8.0.4",
    "npm-run-all2": "^6.0.6",
    "vite": "^4.4.9"
  }
}
